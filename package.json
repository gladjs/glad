    "bluebird": "^3.4.7",
    "body-parser": "1.12.0",
    "chalk": "^1.1.3",
    "debug": "^3.0.1",
    "exists-sync": "0.0.4",
    "express": "^4.14.1",
    "express-session": "1.15.2",
    "express-sessions": "^1.0.6",
    "lodash": "^4.17.4",
    "optimist": "^0.6.1",
    "redis": "2.6.3",
    "sanitizer": "^0.1.3",
    "socket.io": "^4.5.3",
    "unicode": ">= 0.3.1"
  },
  "maintainers": [
    {
      "email": "charliesemailis@gmail.com",
